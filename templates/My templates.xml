<templateSet group="My templates">
  <template name="docf" value="&quot;&quot;&quot; $function_decs$&#10;&#10;Args:&#10;    $arg_name$($arg_type$): $arg_desc$&#10;&#10;Returns:&#10;    $return_type$: $return_desc$&#10;&#10;&quot;&quot;&quot;" description="Docstring for function" toReformat="false" toShortenFQNames="true">
    <variable name="function_decs" expression="pyFunctionName()" defaultValue="&quot;Function decsription&quot;" alwaysStopAt="true" />
    <variable name="arg_name" expression="complete()" defaultValue="&quot;argname&quot;" alwaysStopAt="true" />
    <variable name="arg_type" expression="complete()" defaultValue="&quot;argtype&quot;" alwaysStopAt="true" />
    <variable name="arg_desc" expression="complete()" defaultValue="&quot;argdesc&quot;" alwaysStopAt="true" />
    <variable name="return_type" expression="complete()" defaultValue="&quot;NoneType&quot;" alwaysStopAt="true" />
    <variable name="return_desc" expression="complete()" defaultValue="&quot;return nothing&quot;" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="docfs" value="&quot;&quot;&quot; $function_decs$ &quot;&quot;&quot;" description="Docstring for function" toReformat="false" toShortenFQNames="true">
    <variable name="function_decs" expression="" defaultValue="&quot;Function decsription&quot;" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="titl" value="##################################################&#10;# $text$&#10;##################################################" description="Create title" toReformat="false" toShortenFQNames="true">
    <variable name="text" expression="" defaultValue="Title" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>